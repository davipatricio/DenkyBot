{
  "name": "@denkylabs/bot",
  "version": "0.0.0",
  "description": "Denky is a multipurpose Discord bot used in +3000 servers. Built with Node.js, TypeScript and discord.js.",
  "main": "./src/sharding/shard.js",
  "private": true,
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/denkylabs/denkybot.git"
  },
  "bugs": {
    "url": "https://github.com/denkylabs/denkybot/issues"
  },
  "homepage": "https://github.com/denkylabs/denkybot#readme",
  "scripts": {
    "build": "npx tsc --project tsconfig.json && npx tsc-alias -p tsconfig.json && yarn posttranspile",
    "build:check": "yarn types:check && yarn build",
    "types:check": "npx tsc --noEmit --incremental false",
    "start": "cd ./src && node ./sharding/shard.js",
    "posttranspile": "cd scripts && node copyEnv",
    "lint": "npx eslint src",
    "lint:fix": "yarn lint:prettier && npx eslint src --fix",
    "lint:prettier": "npx prettier -w src"
  },
  "keywords": [],
  "author": "Davi Patricio <veric@denkylabs.com>",
  "contributors": [
    "Nicolas Ribeiro <niskii@denkylabs.com>",
    "Weariful <guiguifeuser@gmail.com>"
  ],
  "license": "GPL-3.0 license",
  "devDependencies": {
    "@types/ioredis": "4.28.10",
    "@types/node": "workspace:^",
    "prisma": "workspace:^",
    "tsc-alias": "1.6.11"
  },
  "dependencies": {
    "@prisma/client": "workspace:^",
    "@sentry/node": "7.6.0",
    "dayjs": "1.11.3",
    "discord.js": "14.0.0-dev.1657325401-33ae7df",
    "dotenv": "16.0.1",
    "fastify": "4.2.0",
    "ioredis": "5.1.0",
    "ms": "2.1.3",
    "prisma-redis-middleware": "4.0.6",
    "tslib": "2.4.0",
    "tweetnacl": "1.0.3",
    "undici": "5.6.1",
    "winston": "3.8.1",
    "winston-transport": "4.5.0"
  }
}
