{
  "name": "@denkylabs/prisma",
  "description": "Module to manage Prisma schemas.",
  "version": "0.0.0",
  "private": true,
  "license": "GPL-3.0 license",
  "scripts": {
    "generate:cockroachdb": "npx prisma generate --schema=./schemas/cockroachdb.prisma",
    "generate:postgres": "npx prisma generate --schema=./schemas/postgres.prisma",
    "generate:mongodb": "npx prisma generate --schema=./schemas/mongodb.prisma",
    "migrate:pg": "npx prisma migrate dev --schema=./schemas/postgres.prisma",
    "migrate:pg:create": "npx prisma migrate dev --name production --schema=./schemas/postgres.prisma --create-only",
    "migrate:cr": "npx prisma migrate dev --schema=./schemas/cockroachdb.prisma",
    "migrate:cr:create": "npx prisma migrate dev --name production --schema=./schemas/cockroachdb.prisma --create-only"
  },
  "author": "Davi Patricio <veric@denkylabs.com>",
  "contributors": [
    "Nicolas Ribeiro <niskii@denkylabs.com>"
  ],
  "devDependencies": {
    "prisma": "workspace:^"
  },
  "dependencies": {
    "@prisma/client": "workspace:^"
  }
}