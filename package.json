{
  "name": "denky-rewrite",
  "version": "1.0.0",
  "description": "",
  "main": "./build/sharding/shard.js",
  "private": true,
  "type": "commonjs",
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/denkylabs/denkybot.git"
  },
  "bugs": {
    "url": "https://github.com/denkylabs/denkybot/issues"
  },
  "scripts": {
    "build": "npx sucrase ./src -d ./build --esModuleInterop --transforms typescript,imports",
    "build:check": "yarn types:check && yarn build",
    "types:check": "npx tsc --noEmit --incremental false",
    "start": "cd ./build && node sharding/shard.js",
    "lint": "npx eslint src",
    "lint:fix": "yarn lint:prettier && npx eslint src --fix",
    "lint:prettier": "npx prettier -w src",
    "generate:postgre": "prisma generate --schema=./src/prisma/postgres.prisma",
    "generate:mongodb": "prisma generate --schema=./src/prisma/mongodb.prisma",
    "migrate": "prisma migrate dev --schema=./src/prisma/postgres.prisma",
    "migrate:create": "prisma migrate dev --name production --schema=./src/prisma/postgres.prisma --create-only"
  },
  "keywords": [],
  "author": "Davi Patricio",
  "license": "GPL-3.0 license",
  "devDependencies": {
    "@types/node": "18.0.1",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "eslint": "8.19.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.7.1",
    "prisma": "4.0.0",
    "sucrase": "3.23.0",
    "typescript": "4.8.0-dev.20220707"
  },
  "dependencies": {
    "@prisma/client": "4.0.0",
    "discord.js": "14.0.0-dev.1657152699-10ba008",
    "dotenv": "16.0.1",
    "fastify": "4.2.0",
    "ms": "2.1.3",
    "tslib": "2.4.0",
    "tweetnacl": "1.0.3",
    "undici": "5.6.0",
    "winston": "3.8.1"
  }
}
